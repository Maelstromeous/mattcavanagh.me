{% extends 'template.twig' %}

{% block title %}Portfolio{% endblock %}

{% block stylesheets %}
<link rel="stylesheet" href="https://cdn.rawgit.com/konpa/devicon/master/devicon.min.css">
{% endblock %}

{% block body %}
<section id="portfolio-index">
    <div class="row no-margin">
        <div class="col-sm-8 col-xs-12 col-sm-offset-2 col-xs-offset-0 text-center">
            <h1>My Portfolio & Skills</h1>
            <p>
                Below you'll find all of my past and current work - projects I feel reflect my skills as a web developer and a DevOps Engineer. There&rsquo;s a mixture of professional, personal and community projects, listing what technologies I&rsquo;ve used, completion dates and links to each (note some links may have changed designs!).
            </p>
            <p>
                Last updated: 10/06/2020
            </p>
        </div>
    </div>
    <div class="row no-margin">
        <div class="col-xs-12 col-sm-5 col-sm-offset-1 text-center">
            <div class="text-tech-icons skills-icons">
                <div class="tech-icon-container">
                    <i class="devicon-amazonwebservices-plain" data-toggle="tooltip" title="AWS"></i>
                    <i class="devicon-angularjs-plain" data-toggle="tooltip" title="AngularJS"></i>
                    <i class="devicon-bootstrap-plain" data-toggle="tooltip" title="Bootstrap 3 / 4 Alpha"></i>
                    <img src="{{ base_url }}/assets/img/bolt-grey.png" data-toggle="tooltip" title="Bolt CMS">
                    <i class="devicon-bower-plain" data-toggle="tooltip" title="Bower"></i>
                    <i class="devicon-docker-plain" data-toggle="tooltip" title="Docker"></i>
                    <i class="devicon-git-plain" data-toggle="tooltip" title="Git"></i>
                    <i class="devicon-github-plain" data-toggle="tooltip" title="GitHub"></i>
                    <i class="devicon-grunt-plain" data-toggle="tooltip" title="Grunt"></i>
                    <i class="devicon-javascript-plain" data-toggle="tooltip" title="JavaScript"></i>
                    <img src="{{ base_url }}/assets/img/jenkins-grey.png" data-toggle="tooltip" title="Jenkins">
                    <i class="devicon-jquery-plain" data-toggle="tooltip" title="jQuery"></i>
                    <i class="fas fa-dharmachakra" data-toggle="tooltip" title="Kubernetes"></i>
                    <i class="devicon-less-plain-wordmark" data-toggle="tooltip" title="LESS"></i>
                    <i class="devicon-linux-plain" data-toggle="tooltip" title="Linux"></i>
                    <img src="{{ asset_url }}/img/materalizecss-grey.png" data-toggle="tooltip" title="Materialize CSS">
                    <i class="devicon-mysql-plain-wordmark" data-toggle="tooltip" title="Mysql / MariaDB"></i>
                    <i class="devicon-nginx-plain" data-toggle="tooltip" title="NGINX Webserver"></i>
                    <i class="devicon-nodejs-plain" data-toggle="tooltip" title="NodeJS"></i>
                    <i class="devicon-photoshop-plain" data-toggle="tooltip" title="Photoshop CS2+"></i>
                    <i class="devicon-php-plain" data-toggle="tooltip" title="PHP 5.5+ / 7"></i>
                    <i class="devicon-redis-plain" data-toggle="tooltip" title="Redis"></i>
                    <img src="{{ asset_url }}/img/terraform.png" data-toggle="tooltip" title="Terraform">
                    <i class="devicon-ubuntu-plain" data-toggle="tooltip" title="Ubuntu 16.04+"></i>
                    <img src="{{ asset_url }}/img/websockets-grey.png" data-toggle="tooltip" title="Websockets">
                </div>
                <p class="caption">
                    All tech I've used in the past and/or continue to use
                </p>
            </div>
        </div>
        <div class="col-xs-12 col-sm-5 text-center">
            <div class="text-tech-icons skills-icons aws">
                <div class="tech-icon-container">
                    <img src="{{ asset_url }}/img/aws/asg.png" data-toggle="tooltip" title="ASG">
                    <img src="{{ asset_url }}/img/aws/aurora.png" data-toggle="tooltip" title="Aurora">
                    <img src="{{ asset_url }}/img/aws/beanstalk.png" data-toggle="tooltip" title="Beanstalk">
                    <img src="{{ asset_url }}/img/aws/cert-manager.png" data-toggle="tooltip" title="Cert Manager (ACM)">
                    <img src="{{ asset_url }}/img/aws/cloudformation.png" data-toggle="tooltip" title="CloudFormation">
                    <img src="{{ asset_url }}/img/aws/cloudwatch.png" data-toggle="tooltip" title="CloudWatch">
                    <img src="{{ asset_url }}/img/aws/codecommit.png" data-toggle="tooltip" title="CodeCommit">
                    <img src="{{ asset_url }}/img/aws/cost-explorer.png" data-toggle="tooltip" title="Cost Explorer">
                    <img src="{{ asset_url }}/img/aws/ebs.png" data-toggle="tooltip" title="EBS">
                    <img src="{{ asset_url }}/img/aws/ec2.png" data-toggle="tooltip" title="EC2">
                    <img src="{{ asset_url }}/img/aws/ecr.png" data-toggle="tooltip" title="ECR">
                    <img src="{{ asset_url }}/img/aws/elasticache.png" data-toggle="tooltip" title="ElastiCache">
                    <img src="{{ asset_url }}/img/aws/elb.png" data-toggle="tooltip" title="ELB / ALB / NLB">
                    <img src="{{ asset_url }}/img/aws/fargate.png" data-toggle="tooltip" title="Fargate">
                    <img src="{{ asset_url }}/img/aws/iam.png" data-toggle="tooltip" title="IAM">
                    <img src="{{ asset_url }}/img/aws/lambda.png" data-toggle="tooltip" title="Lambda">
                    <img src="{{ asset_url }}/img/aws/organisations.png" data-toggle="tooltip" title="Organisations">
                    <img src="{{ asset_url }}/img/aws/rds.png" data-toggle="tooltip" title="RDS">
                    <img src="{{ asset_url }}/img/aws/redshift.png" data-toggle="tooltip" title="Redshift">
                    <img src="{{ asset_url }}/img/aws/reserved-instances.png" data-toggle="tooltip" title="Reserved Instances">
                    <img src="{{ asset_url }}/img/aws/route53.png" data-toggle="tooltip" title="Route53">
                    <img src="{{ asset_url }}/img/aws/s3.png" data-toggle="tooltip" title="S3">
                    <img src="{{ asset_url }}/img/aws/ses.png" data-toggle="tooltip" title="SES">
                    <img src="{{ asset_url }}/img/aws/site-to-sitevpn.png" data-toggle="tooltip" title="VPG / Site to Site VPN">
                    <img src="{{ asset_url }}/img/aws/sns.png" data-toggle="tooltip" title="SNS">
                    <img src="{{ asset_url }}/img/aws/sqs.png" data-toggle="tooltip" title="SQS">
                    <img src="{{ asset_url }}/img/aws/systems-manager.png" data-toggle="tooltip" title="Systems Manager">
                    <img src="{{ asset_url }}/img/aws/vpc.png" data-toggle="tooltip" title="VPC">

                </div>
            </div>
            <p class="caption">
                AWS Services I've used
            </p>
        </div>
    </div>
    <div class="text-center visible-xs row">
        <div class="col-xs-12">
            <p class="no-margin">
                <i class="fa fa-info-circle"></i> To view previews of the sites, simply tap the card. Hover events on mobile and whatnot.
            </p>
        </div>
    </div>
    <div class="text-center">
        <p>
            <i class="fa fa-balance-scale"></i> <small>Legal: For works that are specified as "Professional", I forgo (and have never claimed) any ownership to the work.</small>
        </p>
    </div>
    <div class="row">
        <div class="col-xs-12 hidden-xs hidden-sm">
            <ul class="nav nav-tabs centered-tabs" role="tablist">
                {% include 'portfolio/index/subnav.twig' %}
            </ul>
        </div>
        <div class="col-xs-12 visible-xs visible-sm">
            <ul class="nav nav-pills centered-tabs">
                {% include 'portfolio/index/subnav.twig' %}
            </ul>
        </div>
    </div>

    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="all">
            {% include 'portfolio/index/all.twig' %}
        </div>
        <div role="tabpanel" class="tab-pane" id="featured">
            {% include 'portfolio/index/featured.twig' %}
        </div>
        <div role="tabpanel" class="tab-pane" id="personal">
            {% include 'portfolio/index/personal.twig' %}
        </div>
        <div role="tabpanel" class="tab-pane" id="professional">
            {% include 'portfolio/index/professional.twig' %}
        </div>
    </div>
</section>
{% endblock %}

{% block scripts %}
<script src="{{ base_url }}/assets/js/vendor/jquery.easing.1.3.js"></script>
<script src="{{ base_url }}/assets/js/vendor/imagesloaded.pkgd.min.js"></script>
<script src="{{ base_url }}/assets/js/vendor/masonry.pkgd.min.js"></script>
<script src="{{ base_url }}/assets/js/previews.js{{ version }}"></script>
<script src="{{ base_url }}/assets/js/portfolio.js{{ version }}"></script>
{% endblock %}
